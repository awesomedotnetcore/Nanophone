{
  "title": "Nanophone.AspNetCore.ApplicationServices",
  "version": "1.4.0-alpha",
  "authors": [ "Luk Vermeulen" ],
  "copyright": "Copyright 2016",
  "description": "Nanophone aspnetcore application services",

  "buildOptions": {
    "define": [ "LIBLOG_PORTABLE" ],
    "optimize": true,
    "outputName": "Nanophone.Middleware",
    "xmlDoc": false
  },

  "packOptions": {
    "owners": [ "Luk Vermeulen" ],
    "requireLicenseAcceptance": true,
    "summary": "Nanophone aspnetcore application services",
    "tags": [ "Service", "Discovery", "ServiceDiscovery", "Consul", "Nancy", "WebApi", "Healing", "Autohealing", "Microservices", "aspnetcore", "application services" ],
    "projectUrl": "https://github.com/lvermeulen/Nanophone",
    "licenseUrl": "https://github.com/lvermeulen/Nanophone/blob/master/LICENSE",
    "iconUrl": "http://i.imgur.com/WnKfKOC.png?1",
    "repository": {
      "type": "git",
      "url": "https://github.com/lvermeulen/Nanophone"
    }
  },

  "dependencies": {
    "Microsoft.AspNetCore.Http.Abstractions": "1.0.0",
    "Microsoft.Extensions.DependencyInjection": "1.0.0",
    "Microsoft.Extensions.Options": "1.0.0",
    "Nanophone.Core": { "target": "project" }
  },

  "frameworks": {
    "net451": {
      "dependencies": {
        "Microsoft.AspNet.WebApi": "5.2.3"
      }
    },
    "netstandard1.6": {
      "dependencies": {
        "NETStandard.Library": "1.6.0",
        "System.Dynamic.Runtime": "4.0.11",
        "Microsoft.CSharp": "4.0.1",
        "Microsoft.AspNetCore.Mvc": "1.0.0"
      }
    }
  }
}
